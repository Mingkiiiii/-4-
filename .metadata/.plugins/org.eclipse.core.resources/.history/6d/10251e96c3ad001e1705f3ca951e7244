package ch06_method;

public class MethodMain {

	public static void main(String[] args) {
		//method(메소드) 클래스나 객체에 종속된 함수
		//java는 모두 class에 작성되기 때문에 메소드라 부름
		//1~100까지 더하면 결과는?
		//1~50까지 더하면 결과는?
		//30~60까지 더하면 결과는?
		int sum=0;
		for(int i =1; i<=100; i++) {
			sum+=i;
		}
		System.out.println(sum);

	}

}
