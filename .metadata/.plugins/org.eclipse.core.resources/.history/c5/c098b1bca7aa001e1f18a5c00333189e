package ch05_controll;

public class Loopfor {

	public static void main(String[] args) {
		int num =1;
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		System.out.println("================for문=============");
		// for문
		// (1초기화식; 2조건식 ;4증강식
		for(int i=1; i<=10; i++) {
			System.out.println(i);
		}
		

	}

}
