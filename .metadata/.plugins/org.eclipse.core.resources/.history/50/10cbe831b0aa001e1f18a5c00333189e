package practice;

public class JavaBasicProblem02 {

	public static void main(String[] args) {
		
		        String enigma = "너오구늘리뭐너먹구지리";

		        // "너" 제거
		        String decoded1 = enigma.replace("너", "");

		        // "구" 제거
		        String decoded2 = decoded1.replace("구", "");

		        // "리" 제거
		        String decoded3 = decoded2.replace("리", "");

		        System.out.println("1차 암호 해독[너 제거]: " + decoded1);
		        System.out.println("2차 암호 해독[구 제거]: " + decoded2);
		        System.out.println("3차 암호 해독[리 제거]: " + decoded3);
		        System.out.println("해독완료!! ->" + decoded3);
		    }
		}


	


